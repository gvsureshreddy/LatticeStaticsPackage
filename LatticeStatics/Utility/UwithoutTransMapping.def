   virtual inline int Fsize() {return 6;}
   virtual inline int Ssize() {return DIM3*(InternalAtoms_-1);}
   virtual inline int NoTrans() {return 1;}

   virtual inline int INDF(int i,int j)
   {
      if (i==j)
	 return i;
      else
	 return 2+i+j;
   }

   virtual inline int INDS(int i,int j)
   {
      if (!i)
      {
	 cerr << "Error: INDS(i,j) i==0!!!!!" << endl;
	 exit(-1);
      }
      
      return 6 + (i-1)*3 + j;
   }
   
   virtual inline int INDFF(int k,int l,int m,int n)
   {
      if (k==l)
      {
	 if (m==n)
	 {
	    return DOFS()*k+m;
	 }
	 else
	 {
	    return DOFS()*k + 2+m+n;
	 }
      }
      else
      {
	 if (m==n)
	 {
	    return DOFS()*(2+k+l) + m;
	 }
	 else
	 {
	    return DOFS()*(2+k+l) + 2+m+n;
	 }
      }
   }

   virtual inline int INDSS(int k,int l,int m,int n)
   {
      if (!k || !m)
      {
	 cerr << "Error : INDSS(k,l,m,n) i==0 OR m==0!!!!!!" << endl;
	 exit(-1);
      }
      
      return 6*DOFS() + 6
	 + DOFS()*( (k-1)*3 + l )
	 + ( (m-1)*3 + n );
   }
   
   virtual inline int INDFS(int i,int j,int m,int n)
   {
      if (!m)
      {
	 cerr << "Error : INDFS(i,j,m,n) m==0!!!!!!" << endl;
	 exit(-1);
      }
      
      if (i==j)
      {
	 return DOFS()*i + 6 + (m-1)*3+n;
      }
      else
      {
	 return DOFS()*(2+i+j) + 6 + (m-1)*3+n;
      }
   }
   
   virtual inline int INDSF(int m,int n,int i,int j)
   {
      if (!m)
      {
	 cerr << "Error : INDSF(m,n,i,j) m==0!!!!!!" << endl;
	 exit(-1);
      }
      
      if (i==j)
      {
	 return 6*DOFS() + DOFS()*( (m-1)*3+n ) + i;
      }
      else
      {
	 return 6*DOFS() + DOFS()*( (m-1)*3+n ) + 2+i+j;
      }
   }
